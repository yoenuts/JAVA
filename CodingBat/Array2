
//first problem solved!
class Array2{
    public static void main {
        int[] array;
    }

    public int bigDiff(int[] nums) {
        int largest = nums[0];
        int minimum = nums[0];
        for(int i =1;i < nums.length; i++) {
          largest = Math.max(largest, nums[i]);
          minimum = Math.min(minimum, nums[i]);
        }
        
        return largest - minimum;
    }
}
